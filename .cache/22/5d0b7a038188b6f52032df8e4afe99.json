rn (<circle \n            cx={x}\n            cy={y}\n            r=\"40\"\n            ref={node}\n            {...style} />);\n}\n\nfunction App() {\n    \n    return (<svg width=\"500\" height=\"500\">\n        <DragCircle />\n        <circle \n            cx=\"150\" \n            cy=\"150\" \n            r=\"40\" \n            {...style} />\n    </svg>)\n}\n\n// function \n\nReactDOM.render(<App />, app);"},"lineCount":null}},"error":null,"hash":"fc14211412521c60b913bcc51667585f","cacheData":{"env":{}}}